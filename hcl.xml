clock elapsed_time;

bool all_served() {
    return served[icu] &amp;&amp; served[emergency] &amp;&amp; served[pediatrics];
}

void check_priority() {
    int i;
    for (i = 0; i &lt; humans; i++) {
        if (delivery_ticket_queue[i] &gt; empty) {
            delivery_priority = i;
            return;
        }
    }
    delivery_priority++;
